<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TZTwo.MainPage"
             Title="Main Page">
    <ContentPage.Content>
        <StackLayout Padding="12,12,12,12" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <Button x:Name="Button1" Text="Перейти на страницу 2" Clicked="Button1_Clicked" HorizontalOptions="FillAndExpand"></Button>
            <Button x:Name="Button2" Text="Метоты что использовались" Clicked="Button2_Clicked" HorizontalOptions="Center"></Button>
        </StackLayout>
    </ContentPage.Content>

</ContentPage>
